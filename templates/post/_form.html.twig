{{ form_start(form) }}
<div class="text-danger">
	{{ form_errors(form) }}
</div>
<div class="row">
	<div class="col-md-12">
		{{ form_row(form.titulo) }}
	</div>
</div>
<div class="row">
	<div class="col-md-12">
		{{ form_row(form.descripcion) }}
	</div>
</div>
<div class="row">
	<div class="col-md-12">
		{{ form_row(form.imagen) }}
	</div>
</div>
<div class="row">
	<div class="col-md-12">
		{{ form_row(form.idJuego) }}
	</div>
</div>
<button class="btn btn-primary mt-1 mb-3">{{ button_label|default('Create post') }}</button>
{{ form_end(form) }}
